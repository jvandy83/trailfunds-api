{
	"containerDefinitions": [
		{
			"essential": true,
			"image": "861573826272.dkr.ecr.us-east-2.amazonaws.com/trailfunds-ecr-repo:api",
			"name": "api",
			"portMappings": [
				{
					"containerPort": 5000,
					"hostPort": 5000
				}
			]
		},
		{
			"environment": [
				{
					"name": "POSTGRES_PASSWORD",
					"value": "postgres"
				}
			],
			"essential": true,
			"image": "861573826272.dkr.ecr.us-east-2.amazonaws.com/trailfunds-ecr-repo:postgres",
			"mountPoints": [
				{
					"containerPath": "/docker-entrypoint-initdb.d",
					"sourceVolume": "_DbInitdb"
				},
				{
					"containerPath": "/var/lib/postgresql/data",
					"sourceVolume": "_Postgres"
				}
			],
			"name": "db",
			"portMappings": [
				{
					"containerPort": 5432,
					"hostPort": 5432
				}
			]
		},
		{
			"essential": true,
			"image": "861573826272.dkr.ecr.us-east-2.amazonaws.com/trailfunds-ecr-repo:nginx",
			"name": "nginx",
			"portMappings": [
				{
					"containerPort": 80,
					"hostPort": 80
				}
			]
		}
	],
	"family": "",
	"volumes": [
		{
			"host": {
				"sourcePath": "./db/initdb"
			},
			"name": "_DbInitdb"
		},
		{
			"host": {
				"sourcePath": "./postgres"
			},
			"name": "_Postgres"
		}
	]
}
